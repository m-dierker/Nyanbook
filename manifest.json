{
  "name": "Nyanbook",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "http://facebook.com/*", "https://facebook.com/*", "https://*. facebook.com/*", "http://*.facebook.com/*"
  ],
  "browser_action": {
    "name": "Make Facebook nyan-y",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "nyanbook.png"
  ],
  "manifest_version": 2
}
